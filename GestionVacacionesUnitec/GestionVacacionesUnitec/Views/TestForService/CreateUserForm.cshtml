                        </tr>

                    </table>


                </ul>
            </div>
        </div>
        <div id="site_creacion">
            <h2>Crear Usuario</h2>
            <form>
                <h4>Ingrese talento humano <input placeholder='talento_humano' type='talento_humano' id="talentoHumano"></h4></br></br>
                <h4>Ingrese Email <input placeholder='email' type='email' id="email"></h4></br></br>
                <h4>Ingrese Password <input placeholder='password' type='password' id="password"></h4></br></br>
                <h4>Ingrese Primer nombre <input placeholder='primer_nombre' type='primer_nombre' id="firstName"></h4></br></br>
                <h4>Ingrese Segundo nombre <input placeholder='segundo_nombre' type='segundo_nombre' id="middleName"></h4></br></br>
                <h4>Ingrese Primer apellido  <input placeholder='primer_apellido' type='primer_apellido' id="lastName"></h4></br></br>
                <h4>Ingrese Segundo nombre <input placeholder='segundo_apellido' type='segundo_apellido' id="secondLastName"></h4></br></br>
                <h4>Fecha de ingreso <input placeholder='fecha_ingreso' type='fecha_ingreso' id="employmentDate"></h4></br></br>
                <h4>Fecha de Creacion  <input placeholder='fecha_creacion' type='fecha_creacion' id="signUpDate"></h4></br></br>
                <h4>Activo / Inactivo  <input type="checkbox" value='activo' id="active"></br></br>

            </form>
            ______________________________________________________________________________________________________________________________________

            <h2>Departamento al que pertenece</h2>
            <form action="">
                <h4>Departamento de IT   <input type="checkbox" name="it" value="it" />   Talento Humano   <input type="checkbox" name="th" value="th"></h4>

            </form>

            ______________________________________________________________________________________________________________________________________
            <form action="">
                <h2>Roles</h2>
                <h4>Administrativo <input type="checkbox" name="admi" value="admi" />  Tecnico <input type="checkbox" name="tec" value="tec" /></h4>
                <h4>Catedratico <input type="checkbox" name="cat" value="admi" />  Motorista <input type="checkbox" name="mot" value="mot" /></h4>
            </form>
            <button class='animated pulse' id="submitUser">Crear</button>
        </div>
        <footer><img src="~/Content/images/footer.jpg" /></footer>
       
        <script src="Form.js"></script>
        <!-- javascript en la parte de abajo para que cargue mas rapido -->
